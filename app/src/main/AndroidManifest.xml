<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ssg1.gubba1.gubba1.g">

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="ssg1.gubba1.gubba1.g.AppController"
        android:allowBackup="true"
        android:icon="@mipmap/gg"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/gg"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.onesignal.NotificationAccentColor.DEFAULT"
            android:value="FF00FF00" />
        <activity android:name="ssg1.gubba1.gubba1.g.FrontPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="ssg1.gubba1.gubba1.g.Login"
            android:theme="@style/AppTheme"
            android:launchMode="singleTask"
            />
        <activity
            android:name="ssg1.gubba1.gubba1.g.Home"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme" />
    </application>

</manifest>